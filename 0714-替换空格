public class Solution {
    public static String replaceSpace(String iniString, int length) {
        StringBuffer sb = new StringBuffer();
        for (int i = 0; i < length; i++) {
            if (iniString.charAt(i)==' ') {
                sb.append("%20");
            } else {
                sb.append(iniString.charAt(i));
            }
        }
        return sb.toString();
    }
}
